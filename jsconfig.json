{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2023",
        "moduleResolution": "Classic",
        "allowJs": true,
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "checkJs": true
    },
    "compileOnSave": true,
    "typingOptions": {
        "enableAutoDiscovery": true
    }
}
